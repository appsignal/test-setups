{
    "name": "appsignal/laravel",
    "description": "AppSignal for Laravel",
    "version": "0.1.0",
    "require": {
		"open-telemetry/exporter-otlp": "^1.3",
		"open-telemetry/opentelemetry-auto-laravel": "^1.4",
		"open-telemetry/sdk": "^1.9",
		"open-telemetry/opentelemetry-auto-io": "^0.1.0",
		"vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
		"psr-4": {
			"AppSignal\\LaravelAppSignal\\": "src"
		},
		"files": [
			"bootstrap/autoload.php"
		]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "tbachert/spi": false
        }
    }
}
